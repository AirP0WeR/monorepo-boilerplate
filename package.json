{
  "name": "monorepo-boilerplate",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "cp .env.development .env && FORCE_COLOR=1 dotenv -e .env -- turbo run dev --filter=web --filter=worker",
    "build": "cp .env.production .env && dotenv -e .env -- turbo run build --filter=web --filter=worker --no-daemon",
    "start": "cp .env.production .env && dotenv -e .env -- turbo run start --no-daemon",
    "clean": "turbo run clean && rm -rf node_modules && rm -rf .turbo",
    "lint": "turbo run lint --no-daemon",
    "typecheck": "turbo run typecheck --no-daemon",
    "prepare": "husky"
  },
  "type": "module",
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "dotenv-cli": "7.3.0",
    "husky": "8.0.0",
    "prettier-plugin-tailwindcss": "0.5.11",
    "semantic-release": "23.0.2",
    "turbo": "1.11.3",
    "typescript": "5.3.3"
  },
  "keywords": [],
  "author": "airp0wer",
  "license": "MIT"
}
